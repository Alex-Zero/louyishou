page {
    position: relative;
    background: #f4f4f4;
}

.orders {
    height: auto;
    width: 100%;
    overflow: hidden;
}

.order {
    margin-top: 20rpx;
    background: #fff;
    height: 90%;
}

.info-no {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    padding: 10rpx;
    font-size: 28rpx;
    color: #666;
}

.order .h {
    height: 83.3rpx;
    line-height: 83.3rpx;
    margin-left: 31.25rpx;
    padding-right: 31.25rpx;
    border-bottom: 1px solid #f4f4f4;
    font-size: 30rpx;
    color: #333;
}

.order .h .l {
    float: left;
}

.order .h .r {
    float: right;
    color: #b4282d;
    font-size: 24rpx;
}

.order .goods {
    display: flex;
    align-items: center;
    height: 189rpx;
    margin-left: 31.25rpx;
}

.order .goods .img {
    height: 145.83rpx;
    width: 145.83rpx;
    background: #f4f4f4;
}

.order .goods .img image {
    height: 145.83rpx;
    width: 145.83rpx;
}

.order .goods .info {
    height: 145.83rpx;
    flex: 1;
    padding-left: 20rpx;
}

.order .goods .name {
    margin-top: 10rpx;
    display: block;
    height: 44rpx;
    line-height: 44rpx;
    color: #333;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 520rpx;
}

.order .goods .number {
    display: block;
    height: 37rpx;
    line-height: 37rpx;
    color: #666;
    font-size: 25rpx;
}

.order .goods .status {
    width: 105rpx;
    color: #b4282d;
    font-size: 25rpx;
}

.order .b {
    height: 103rpx;
    line-height: 103rpx;
    margin-left: 31.25rpx;
    padding-right: 31.25rpx;
    border-top: 1px solid #f4f4f4;
    font-size: 30rpx;
    color: #333;
}

.order .b .l {
    float: left;
}

.order .b .r {
    float: right;
}

.order .b .btn {
    margin-top: 19rpx;
    height: 64.5rpx;
    line-height: 64.5rpx;
    text-align: center;
    padding: 0 20rpx;
    border-radius: 5rpx;
    font-size: 28rpx;
    color: #fff;
    background: #b4282d;
}

.order_div {
    display: flex;
    width: 94%;
    margin: 0rpx auto;
}

.order_right {
    font-size: 18px;
    font-weight: bold;
    color: #f37b25;
    text-align: right;
}

dl dd,dt {
    width: 100%;
    display: block;
}

dt {
    margin-bottom: 15rpx;
    font-size: 26rpx;
    color: #8a8c8e;
}

dd {
    color: #666;
    font-size: 13px;
}

.order_status {
    margin-top: 5rpx;
    height: 50rpx;
    display: flex;
    justify-content: space-between;
    border-top: 1px solid #eee;
}

.order_status {
    margin: 5rpx 0 20rpx 0;
    height: 50rpx;
    display: flex;
    justify-content: space-between;
}

.order_status_left {
    margin: 18rpx 25rpx;
}

.order_status_right {
    padding-top: 13rpx;
    margin-right: 10rpx;
    display: flex;
}

.order_status_button {
    border: 1px solid #b4282d;
    height: 50rpx;
    padding: 0 25rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 25rpx;
    color: #b4282d;
    margin-right: 10rpx;
}

.item-c {
    padding-left: 31.25rpx;
    border-top: 1px solid #f4f4f4;
    height: 103rpx;
    line-height: 103rpx;
    background: #fff;
}

.item-c .l {
    float: left;
}

.item-c .r {
    height: 103rpx;
    float: right;
    display: flex;
    align-items: center;
    padding-right: 16rpx;
}

.item-c .r .btn {
    float: right;
}

.item-c .cost {
    color: #b4282d;
}

.item-c .btn {
    line-height: 66rpx;
    border-radius: 5rpx;
    text-align: center;
    margin: 0 10rpx;
    padding: 0 20rpx;
    height: 66rpx;
}

.item-c .btn.active {
    background: #b4282d;
    color: #fff;
}

.blank {
    width: 100%;
    height: 90rpx;
}

.cate_bg {
    width: 100%;
    height: 70rpx;
    background: #b4282d;
}

.cate {
    display: flex;
    font-size: 29rpx;
    background: #fff;
    flex-wrap: wrap;
    color: #666;
    position: absolute;
    width: 100%;
}

.cate-item {
    text-align: center;
    width: 20%;
    margin-top: 20rpx;
    padding-bottom: 20rpx;
}

.hover {
    color: #b4282d;
    width: 100rpx;
    margin: 0 auto;
    margin-top: 20rpx;
    border-bottom: 2px solid #b4282d;
    padding-bottom: 20rpx;
}

.order-goods {
    background: #fff;
    margin-top: 20rpx;
}

.order-goods .h {
    height: 93.75rpx;
    line-height: 93.75rpx;
    margin-left: 31.25rpx;
    border-bottom: 1px solid #f4f4f4;
    padding-right: 31.25rpx;
}

.order-goods .h .label {
    float: left;
    font-size: 29rpx;
    color: #333;
    width: 100%;
}

.order-goods .h .status {
    float: right;
    font-size: 29rpx;
    color: #b4282d;
}

.order-goods .item {
    display: flex;
    align-items: center;
    height: 192rpx;
    margin-left: 31.25rpx;
    padding-right: 31.25rpx;
    border-bottom: 1px solid #f4f4f4;
    width: 100%;
}

.order-goods .item:last-child {
    border-bottom: none;
}

.order-goods .item .img {
    height: 145.83rpx;
    width: 145.83rpx;
    background: #f4f4f4;
}

.order-goods .item .img image {
    height: 145.83rpx;
    width: 145.83rpx;
}

.order-goods .item .info {
    flex: 1;
    height: 145.83rpx;
    margin-left: 20rpx;
}

.order-goods .item .t {
    margin-top: 8rpx;
    height: 33rpx;
    line-height: 33rpx;
    margin-bottom: 10.5rpx;
}

.order-goods .item .t .name {
    display: block;
    float: left;
    height: 33rpx;
    line-height: 33rpx;
    color: #333;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 500rpx;
}

.order-goods .item .t .number {
    display: block;
    float: right;
    height: 33rpx;
    text-align: right;
    line-height: 33rpx;
    color: #333;
    font-size: 30rpx;
    padding-right: 31.25rpx;
}

.order-goods .item .attr {
    height: 29rpx;
    line-height: 29rpx;
    color: #666;
    margin-bottom: 25rpx;
    font-size: 25rpx;
}

.order-goods .item .price {
    height: 30rpx;
    line-height: 30rpx;
    color: #333;
    font-size: 30rpx;
}