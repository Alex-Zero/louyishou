page {
    height: 100%;
    width: 100%;
    background: #f4f4f4;
}

.container {
    background: #fff;
    height: auto;
    overflow: hidden;
    width: 100%;
}

.fansbox {
    width: 100%;
    height: 200rpx;
    margin-bottom: 20rpx solid #eee;
}

.fansbox image {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
    background: #f4f4f4;
}

.newtop {
    height: 260rpx;
    width: 100%;
    background: #b4282d;
    background-image: -webkit-gradient(linear,50% 0,0 100%,from(transparent),color-stop(0.5,transparent),color-stop(0.5,#fff),to(#fff)),-webkit-gradient(linear,50% 0,100% 100%,from(transparent),color-stop(0.5,transparent),color-stop(0.5,#fff),to(#fff));
    background-image: -moz-linear-gradient(50% 0 -45deg,transparent,transparent 50%,#fff 50%,#fff),-moz-linear-gradient(50% 0 -135deg,transparent,transparent 50%,#fff 50%,#fff);
    background-size: 20rpx 10rpx;
    background-repeat: repeat-x;
    background-position: 0 100%;
    box-shadow: 3px 3px 10px rgba(0,0,0,0.2);
}

.newtop1 {
    height: 110rpx;
    width: 100%;
    background: #fff;
    position: relative;
}

.posi {
    position: absolute;
    top: -200rpx;
    width: 90%;
    border-radius: 10rpx;
    height: 280rpx;
    background: #fff;
    box-shadow: 3px 3px 10px rgba(0,0,0,0.2);
    left: 5%;
}

.guajian {
    display: flex;
    flex-direction: row;
    width: 100%;
    padding: 30.25rpx;
}

.copy-right {
    display: flex;
    justify-content: center;
    width: 100%;
    font-size: 26rpx;
    color: #121212;
    margin-top: 20rpx;
}

.copy-right-1 {
    display: flex;
    justify-content: center;
    width: 100%;
    font-size: 26rpx;
    color: #121212;
    margin-top: 40rpx;
    margin-bottom: 40rpx;
}

button {
    padding: 0;
    margin: 0;
}

button::after {
    content: none;
}

.profile-info {
    width: 100%;
    height: 180rpx;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    justify-content: flex-start;
    padding: 0 30.25rpx;
    border-radius: 10rpx;
}

.profile-info .avatar {
    height: 125rpx;
    width: 125rpx;
    border-radius: 50%;
}

.profile-info .info {
    flex: 1;
    height: 125rpx;
    padding-left: 31.25rpx;
}

.profile-info .name {
    background: rgba(0,0,0,0);
    padding: 0;
    margin: 0;
    text-align: left;
    color: #333;
    font-size: 35rpx;
}

.profile-info .level {
    background: rgba(0,0,0,0);
    display: block;
    height: 30rpx;
    line-height: 30rpx;
    font-size: 28rpx;
    margin-bottom: 10rpx;
    color: #333;
    text-align: left;
}

.user-menu {
    width: 100%;
    height: auto;
    overflow: hidden;
    background: #fff;
}

.user-menu .item {
    float: left;
    width: 33.33333%;
    height: 187.5rpx;
    border-right: 1px solid rgba(0,0,0,0.15);
    border-bottom: 1px solid rgba(0,0,0,0.15);
    text-align: center;
}

.user-menu .item .a {
    display: flex;
    width: 100%;
    height: 100%;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.user-menu .item .a image {
    width: 75rpx;
    height: 75rpx;
    margin-bottom: 10rpx;
}

.user-menu .item.no-border {
    border-right: 0;
}

.user-menu .item.item-bottom {
    border-bottom: none;
}

.user-menu .icon {
    margin: 0 auto;
    display: block;
    height: 52.803rpx;
    width: 52.803rpx;
    margin-bottom: 16rpx;
}

.user-menu .icon.order {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -437.5rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.coupon {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -62.4997rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.gift {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -187.5rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.address {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 0 no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.security {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -500rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.kefu {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -312.5rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.help {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -250rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.feedback {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -125rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .txt {
    display: block;
    height: 24rpx;
    width: 100%;
    font-size: 24rpx;
    color: #333;
}

.logout {
    margin-top: 50rpx;
    height: 101rpx;
    width: 100%;
    line-height: 101rpx;
    text-align: center;
    background: #fff;
    color: #333;
    font-size: 30rpx;
}

.user-article {
    background-color: rgb(255,255,255);
}

.user-order {
    display: flex;
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    padding: 0 20rpx;
    border-bottom: 1rpx solid rgb(216,216,216);
    box-sizing: border-box;
}

.order-all {
    width: 630rpx;
    font-size: 28rpx;
    color: rgb(102,102,102);
}

.order-look {
    font-size: 24rpx;
    color: rgb(153,153,153);
}

.icon-right {
    display: inline-block;
    vertical-align: -4rpx;
}

.order-class {
    display: flex;
    width: 100%;
    box-sizing: border-box;
    justify-content: space-between;
}

.item-order {
    width: 25%;
    height: 155rpx;
    text-align: center;
    color: rgb(153,153,153);
}

.item-order .iconfont {
    width: 80rpx;
    height: 80rpx;
    margin: 0 auto;
}

.item--order-text {
    line-height: 34rpx;
    font-size: 24rpx;
}