<view class="container">
    <view class="brand-list">
        <navigator class="item" url="../brandDetail/brandDetail?id={{item.id}}" wx:for="{{brandList}}" wx:key="id">
            <view class="img-bg">
                <image backgroundSize="cover" src="{{item.info_pic_url}}"></image>
            </view>
            <view class="txt-box">
                <view class="line">
                    <text class="name">{{item.name}}</text>
                    <text class="s">|</text>
                    <text class="price">{{item.floor_price}}元起</text>
                </view>
            </view>
        </navigator>
    </view>
</view>
